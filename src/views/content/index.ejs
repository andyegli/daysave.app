<!DOCTYPE html>
<html>
<head>
  <title>Content</title>
</head>
<body>
  <h1>Content List</h1>
  <a href="/content/create">Create New Content</a>
  <ul>
    <% contents.forEach(content => { %>
      <li>
        <a href="/content/<%= content.id %>"><%= content.title %></a>
        <a href="/content/<%= content.id %>/edit">Edit</a>
        <form action="/content/<%= content.id %>/delete" method="POST" style="display:inline;">
          <button type="submit">Delete</button>
        </form>
      </li>
    <% }); %>
  </ul>
</body>
</html>